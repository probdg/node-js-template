// User model
model User {
  id            String   @id @default(uuid())
  email         String   @unique
  username      String   @unique
  password_hash String   @default("")
  role          String   @default("user")
  is_active     Boolean  @default(true)
  created_at    DateTime @default(now())
  updated_at    DateTime @updatedAt

  @@map("users")
}
